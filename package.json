{
	"name": "react-play-server",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "node backend/server.js",
		"server": "nodemon backend/server.js",
		"client": "npm run dev --prefix frontend",
		"dev": "concurrently \"npm run server\" \"npm run client\""
	},
	"nodemonConfig": {
		"ignore": [
			"frontend/*"
		]
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.441.0",
		"@mux/mux-node": "^7.3.2",
		"bcrypt": "^5.1.1",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^16.3.1",
		"exceljs": "^4.4.0",
		"express": "^4.18.2",
		"google-auth-library": "^9.2.0",
		"http-errors": "~1.6.3",
		"jsonwebtoken": "^9.0.2",
		"livekit-server-sdk": "^1.2.7",
		"mongoose": "^7.6.4",
		"morgan": "~1.9.1",
		"multer": "^1.4.5-lts.1",
		"node-html-to-image": "^4.0.0",
		"nodemailer": "^6.9.7",
		"nodemon": "^3.0.1",
		"slugify": "^1.6.6",
		"socket.io": "^4.7.2",
		"stripe": "^14.3.0",
		"validator": "^13.11.0"
	},
	"devDependencies": {
		"concurrently": "^8.2.2"
	}
}
